# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'DrCRYPTO' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DrCRYPTO

    # Networking Pods
    pod 'Alamofire'
    pod 'SwiftyJSON'
    pod 'Kingfisher'
    pod 'ObjectMapper'
    pod 'AlamofireObjectMapper'

    # DAPP Pods
    pod 'web3swift'
    
    # UI Pods
    pod 'XLPagerTabStrip'

  target 'DrCRYPTOTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'DrCRYPTOUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
